<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/config_nav_graph"
    app:startDestination="@id/principal">

    <fragment
        android:id="@+id/principal"
        android:name="com.example.kotlinobservalo.Config.fragments.Principal"
        android:label="fragment_principal"
        tools:layout="@layout/fragment_principal" >
        <action
            android:id="@+id/action_principal_to_teclado"
            app:destination="@id/teclado" />
    </fragment>
    <fragment
        android:id="@+id/teclado"
        android:name="com.example.kotlinobservalo.Config.fragments.Teclado"
        android:label="fragment_teclado"
        tools:layout="@layout/fragment_teclado" />
</navigation>